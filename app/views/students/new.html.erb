<h1>Student Form</h1>


<%= form_tag students_path do %>
    <%= label_tag("student[first_name]", "First Name:")%>
        <br>
        <%= text_field_tag "student[first_name]", @student.first_name, id: "first_name" %> 
        <br>
        <%= label_tag("student[last_name]", "Last Name:")%><br>
        <%= text_field_tag "student[last_name]", @student.last_name, id: "last_name" %>
        <br>
        <input_type="text" id="last_name" name="student[last_name]">
        <br>
        <input type="submit" value="Submit Student">

        
    <%end%>